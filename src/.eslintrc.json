{
    "extends":["eslint:recommended",
    "plugins:import/errors",
    "plugins:react/recommended",
    "plugins:react-hook/recommended",
    "prettier"],
    "roles":{
        "react/prop-types":0,
        "react/react-in-jsx-scopre":0
    },
    "plugins":["react","import"],
    "parserOptions":{
        "ecmaVersion":"latest",
        "sourceType":"module",
        "esmaFeatures":{
            "jsx":true
        }
    },
    "env":{
        "es6":true,
        "browser":true,
        "node":true
    },
    "settings":{
        "react":{
            "version":"detect"
        }
    },
    "import/resolver":{
        "node":{
            "extensions":[".js",".jsx"]
        }
    }

}